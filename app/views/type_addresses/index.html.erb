<h1>Listing type_addresses</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Inactivated at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @type_addresses.each do |type_address| %>
  <tr>
    <td><%= type_address.description %></td>
    <td><%= type_address.inactivated_at %></td>
    <td><%= link_to 'Show', type_address %></td>
    <td><%= link_to 'Edit', edit_type_address_path(type_address) %></td>
    <td><%= link_to 'Destroy', type_address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

